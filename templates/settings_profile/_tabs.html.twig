
{% embed "shared/_tabs.html.twig" %}
    {% block tabs %}
        {% embed "shared/_tab.html.twig" with {route:"app_settings_profile" } %}
            {% block content %}
                Settings
            {% endblock %}
            {% block path %}
                {{ path('app_settings_profile') }}
            {% endblock %}
        {% endembed %}

        {% embed "shared/_tab.html.twig" with {route:"app_settingsprofile_profileimage",classes:"ml-2" } %}
            {% block content %}
                Avatar Upload Image
            {% endblock %}
            {% block path %}
                {{ path('app_settingsprofile_profileimage') }}
            {% endblock %}
        {% endembed %}

    {% endblock %}
{% endembed %}
